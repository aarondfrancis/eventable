{
    "name": "aaronfrancis/eventable",
    "description": "A Laravel package for tracking events on Eloquent models using polymorphic relationships",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aarondfrancis@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "nesbot/carbon": "^3.0",
        "staudenmeir/laravel-cte": "^1.6"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0|^4.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AaronFrancis\\Eventable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronFrancis\\Eventable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronFrancis\\Eventable\\EventableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "pest",
        "lint": "pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
